{
  "name": "cassettenest",
  "version": "4.5.0",
  "description": "A way for analog photographers to track their film use from box to binder.",
  "scripts": {
    "watch:tailwind": "postcss tailwind/main.css -o static/tailwind.css --watch --verbose",
    "build:tailwind": "NODE_ENV=production postcss tailwind/main.css -o static/tailwind.css",
    "dj": "python manage.py runserver",
    "start": "npm-run-all --parallel watch:tailwind dj",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trey/cassettenest.git"
  },
  "keywords": [],
  "author": "Trey Piepmeier <trey@treypiepmeier.com> (https://piep.works/)",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/trey/cassettenest/issues"
  },
  "homepage": "https://github.com/trey/cassettenest#readme",
  "devDependencies": {
    "@playwright/test": "^1.36.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.12",
    "cssnano": "^6.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.33.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^11.0.0",
    "stylelint": "^16.0.1",
    "stylelint-config-standard": "^35.0.0",
    "tailwindcss": "^3.2.4"
  }
}
