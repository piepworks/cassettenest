{
    "editor.formatOnSave": false,
    "[javascript]": {
        "editor.formatOnSave": true,
    },
    "[python]": {
        "editor.formatOnSave": true,
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.stylelint": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "files.associations": {
        "*.html": "django-html",
        "*.js": "javascript",
        "*.svg": "django-html",
    },
    "python.analysis.exclude": ["venv"],
}
